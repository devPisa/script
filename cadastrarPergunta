from faker import Faker;
import secrets;
import lorem;

fake = Faker('pt_BR')
def cadastrarPergunta():
    id_pergunta = fake.random_int();
    pergunta = secrets.token_urlsafe(64);
    documento = fake.random_int(0,1);

    print(f"id_pergunta:{id_pergunta}");
    print(f"Pergunta:{pergunta}");
    print(f"Documento:{documento}");

cadastrarPergunta();
